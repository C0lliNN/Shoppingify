{
  "name": "shoppingify",
  "version": "1.0.0",
  "description": "App to manage shop lists",
  "main": "index.js",
  "engine": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "lint": "eslint server",
    "test-server": "jest server --watch --detectOpenHandles --coverage",
    "test": "concurrently \"npm run testServer\"  \"npm run test a --prefix client\""
  },
  "author": "Raphael Collin",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.2.0",
    "faker": "^5.1.0",
    "helmet": "^4.1.0",
    "joi": "^17.2.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "saslprep": "^1.0.3",
    "supports-color": "^7.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
